{
  "name": "{{PROJECT_NAME}}",
  "version": "1.0.0",
  "description": "Project built with duEuler Foundation",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
